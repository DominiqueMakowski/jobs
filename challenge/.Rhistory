df1$Rating_Intensity <- df1$Rating_Intensity + 3
df2 <- correlation::simulate_simpson(n=5, groups=20)
names(df2) <- c("Rating_Intensity", "Rating_SelfRelevance", "Participant")
df2$Condition <- "Neutral"
df2$Rating_Intensity <- df2$Rating_Intensity -3
df <- rbind(df1, df2) %>%
arrange(Participant, Condition)
df$Rating_SelfRelevance <- df$Rating_SelfRelevance + 10
df %>%
ggplot(aes(x=Rating_SelfRelevance, y=Rating_Intensity, shape=Condition, color=Participant)) +
geom_point(size=3) +
geom_smooth(method="lm")
t.test(df$Rating_Intensity ~ df$Condition)
t.test(df$Rating_SelfRelevance ~ df$Condition)
parameters(lme4::lmer(Rating_Intensity ~ Condition + (1|Participant), data=df))
parameters(lme4::lmer(Rating_Intensity ~ Rating_SelfRelevance * Condition + (1|Participant), data=df))
parameters(lme4::lmer(Rating_SelfRelevance ~ Condition + (1|Participant), data=df))
